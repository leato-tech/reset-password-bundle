{
    "name": "leato-tech/reset-password-bundle",
    "description": "Symfony bundle that adds password reset functionality.",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "symfony/config": "^6.0 | ^7.0",
        "symfony/dependency-injection": "^6.0 | ^7.0",
        "symfony/deprecation-contracts": "^2.2 | ^3.0",
        "symfony/http-kernel": "^6.0 | ^7.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.7",
        "symfony/framework-bundle": "^6.0 | ^7.0",
        "symfony/phpunit-bridge": "^6.0 | ^7.0",
        "doctrine/doctrine-bundle": "2.11.1",
        "doctrine/annotations": "2.0.1"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2.0"
    },
    "autoload": {
        "psr-4": {
            "SymfonyCasts\\Bundle\\ResetPassword\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SymfonyCasts\\Bundle\\ResetPassword\\Tests\\": "tests/"
        }
    }
}
